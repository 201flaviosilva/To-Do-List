  "scripts": {
    "clear": "rm -rf build && rm -rf out && rm -rf dist",
    "dev": "vite",
    "start": "vite --open",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint:check": "eslint ./src/",
    "lint": "eslint ./ --fix",
    "analyze": "npm run build && source-map-explorer 'dist/js/*.js' --html ./source-map-explorer/result.html && source-map-explorer 'dist/js/*.js' --json ./source-map-explorer/result.json",
    "depcheck": "depcheck",
    "bump": "npm run build && np --no-tests --any-branch --no-publish"
  },
  "dependencies": {
    "css-free-style": "^1.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.0",
    "styled-components": "^6.1.17",
    "sweetalert2": "^11.17.2",
    "utilidades": "^1.4.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "depcheck": "^1.4.7",
    "eslint": "^9.19.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
