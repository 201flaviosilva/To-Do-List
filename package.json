    "np": "^10.2.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.3.4"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/201flaviosilva/To-Do-List.git"
  },
